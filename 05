#include<stdio.h>
int main (void) {
     printf(" hello everyone "); 
     printf(" my height is %d m %d cm \n",1,60);
     printf(" this building is %05d cm \n", 1000);
     printf(" this building is %06d cm \n", 7890);
     printf(" this building is %06d cm \n", 67890);
     printf(" this building is %06d cm \n", 567890);
     printf(" this building is %06d cm \n", 1234567890);
     printf(" pi = %1.10f \n ", 3.1415926535897932384626433832795);
}
